# Database Configuration
DB_URL=jdbc:postgresql://localhost:5432/minha_app_db
DB_USER=postgres
DB_PASSWORD=123456

# RabbitMQ Configuration (External Python Service)
# Use localhost if RabbitMQ is running on the same host
# Use host.docker.internal if connecting from Docker container to host
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=guest
RABBITMQ_PASS=guest

# Email Configuration (Mailtrap or other provider)
MAIL_HOST=smtp.mailtrap.io
MAIL_PORT=587
MAIL_USER=your_mailtrap_username
MAIL_PASS=your_mailtrap_password

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_here_make_it_long_and_secure_at_least_256_bits

# Spring Profile (optional)
SPRING_PROFILES_ACTIVE=development
